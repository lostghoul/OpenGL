<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:a="http://schemas.android.com/apk/res/android"
    package="rk.or.android"
    a:versionCode="4"
    a:versionName="OriSim3D-1.4">

    <uses-sdk
        a:minSdkVersion="7"
        a:targetSdkVersion="18"></uses-sdk>
    <supports-screens
        a:anyDensity="true"
        a:largeScreens="true"
        a:normalScreens="true"
        a:resizeable="true"
        a:smallScreens="true"></supports-screens>
    <uses-feature
        a:glEsVersion="0x00020000"
        a:required="true" />
    <application
        a:allowBackup="true"
        a:allowClearUserData="true"
        a:icon="@drawable/ic_launcher_orisim"
        a:label="@string/orisim3d_app"
        a:process="rk.or.android.ModelSelection">
        <activity
            a:name="ModelSelection"
            a:noHistory="false"
            a:stateNotNeeded="true">
            <intent-filter>
                <action a:name="android.intent.action.MAIN" />
                <category a:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            a:name="ModelView"
            a:exported="false"
            a:noHistory="true"
            a:stateNotNeeded="true">
            <intent-filter>
                <action a:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>
</manifest>
